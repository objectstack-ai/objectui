            { "id": "4", "name": "Initech", "status": "active", "email": "pr@initech.com", "amount": 3400, "createdAt": "2023-04-20" },
            { "id": "5", "name": "Umbrella Corp", "status": "active", "email": "secure@umbrella.com", "amount": 9900, "createdAt": "2023-05-12" }
