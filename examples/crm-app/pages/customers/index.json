{
  "type": "page",
  "title": "Customers",
  "body": [
    {
      "type": "card",
      "className": "p-0",
      "children": [
        {
          "type": "data-table",
          "toolbar": [
            {
              "type": "flex",
              "justify": "between",
              "className": "p-4 border-b",
              "children": [
                {
                  "type": "div",
