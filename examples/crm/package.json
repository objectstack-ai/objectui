{
  "name": "@object-ui/example-crm",
  "version": "0.1.0",
  "description": "CRM Metadata Example using ObjectStack Protocol",
  "private": true,
  "main": "dist/objectstack.json",
  "types": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./objectstack.config": "./objectstack.config.ts"
  },
  "scripts": {
    "serve": "objectstack serve objectstack.config.ts",
    "build": "objectstack compile objectstack.config.ts dist/objectstack.json",
    "start": "tsx server.ts"
  },
  "dependencies": {
    "@objectstack/core": "^0.9.0",
    "@objectstack/runtime": "^0.9.0",
    "@objectstack/spec": "^0.9.0",
    "pino": "^8.21.0"
  },
  "devDependencies": {
    "@objectstack/cli": "^0.9.0",
    "typescript": "^5.0.0"
  }
}
