{
  "name": "@object-ui/cli",
  "version": "0.3.0",
  "description": "CLI tool for Object UI - Build applications from JSON schemas",
  "type": "module",
  "bin": {
    "objectui": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src"
  },
  "keywords": [
    "object-ui",
    "cli",
    "schema-driven",
    "ui-builder",
    "json"
  ],
  "author": "ObjectQL Team",
  "license": "MIT",
  "dependencies": {
    "@object-ui/components": "workspace:*",
    "@object-ui/react": "workspace:*",
    "@types/glob": "^9.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "chalk": "^5.4.1",
    "commander": "^14.0.2",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "glob": "^13.0.0",
    "js-yaml": "^4.1.0",
    "vite": "^7.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.8",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3"
  }
}
